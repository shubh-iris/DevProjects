,"original":{"line":35,"column":35}},{"generated":{"line":103,"column":0},"source":"components/App.js","original":{"line":35,"column":8}},{"generated":{"line":103,"column":8},"source":"components/App.js","original":{"line":31,"column":6}},{"generated":{"line":103,"column":9},"source":"components/App.js","original":{"line":30,"column":4}},{"generated":{"line":104,"column":0},"source":"components/App.js","original":{"line":38,"column":3}},{"name":"Component","generated":{"line":108,"column":2},"source":"components/App.js","original":{"line":8,"column":18}},{"name":"App","generated":{"line":110,"column":15},"source":"components/App.js","original":{"line":41,"column":15}}],"sources":{"components/App.js":"import React, { Component } from \"react\";\nimport Search from \"./Search\";\nimport Artist from \"./Artist\";\nimport Tracks from \"./Tracks\";\n\nconst API_ADDRESS = \"https://spotify-api-wrapper.appspot.com\";\n\nclass App extends Component {\n  state = { artist: null, tracks: [] };\n\n  searchArtist = (artistQuery) => {\n    fetch(`${API_ADDRESS}/artist/${artistQuery}`)\n      .then((response) => response.json())\n      .then((json) => {\n        if (json.artists.total > 0) {\n          const artist = json.artists.items[0];\n\n          this.setState({ artist });\n\n          fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n            .then((response) => response.json())\n            .then((json) => this.setState({ tracks: json.tracks }))\n            .catch((error) => alert(error.message));\n        }\n      })\n      .catch((error) => alert(error.message));\n  };\n\n  render() {\n    return (\n      <div>\n        <h2>Music Master</h2>\n        <Search searchArtist={this.searchArtist} />\n        <Artist artist={this.state.artist} />\n        <Tracks tracks={this.state.tracks} />\n      </div>\n    );\n  }\n}\n\nexport default App;\n"},"lineCount":null}},"hash":"e0ebd2f307959473e3b6bf4353d6d699","cacheData":{"env":{}}}